{
    "name": "SyedSohail9t5/laravel-auto-translate",
    "description": "Automatically translate your language files using Google Translate",
    "keywords": [
        "Sohail9t5",
        "laravel-auto-translate"
    ],
    "homepage": "https://github.com/SyedSohail9t5/laravel-auto-translate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Syed Sohail",
            "email": "sohail.haider@sociabletech.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "*",
        "google/cloud": "^0.105.0",
        "illuminate/support": "*",
        "scn/deepl-api-connector": "^1.3",
        "stichoza/google-translate-php": "^4.0",
        "themsaid/laravel-langman": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.* || 3.7.* || 3.8.*",
        "phpunit/phpunit": "*",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sohail9t5\\AutoTranslate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sohail9t5\\AutoTranslate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sohail9t5\\AutoTranslate\\AutoTranslateServiceProvider"
            ],
            "aliases": {
                "AutoTranslate": "Sohail9t5\\AutoTranslate\\AutoTranslateFacade"
            }
        }
    }
}
